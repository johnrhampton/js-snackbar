{
  "name": "js-snackbar",
  "version": "0.1.7",
  "description": "Custom Material Design inspired SnackBar notifications",
  "main": "src/js/snackbar.js",
  "scripts": {
    "dev": "npm3 install && webpack-dev-server --config webpack.config.js --content-base build/ --inline --hot",
    "dist": "npm3 install && npm3 prepublish && webpack --config webpack.deploy.config.js",
    "prepublish": "babel ./src/snackbar --out-dir ./src/lib --presets es2015"
  },
  "devDependencies": {
